module tkd.element.widget.state;

/**
 * State values of widgets.
 */
enum State : string
{
	normal     = "normal",
	active     = "active",
	disabled   = "disabled",
	focus      = "focus",
	pressed    = "pressed",
	selected   = "selected",
	background = "background",
	readonly   = "readonly",
	alternate  = "alternate",
	invalid    = "invalid",
	hover      = "hover",
}

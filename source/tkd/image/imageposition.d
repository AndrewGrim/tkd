module tkd.image.imageposition;

/**
 * Position of image relative to the text.
 */
enum ImagePosition : string
{
	text   = "text",
	image  = "image",
	center = "center",
	top    = "top",
	bottom = "bottom",
	left   = "left",
	right  = "right",
	none   = "none",
}
